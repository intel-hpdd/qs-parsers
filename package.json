    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "flow-bin": "^0.49.1",
    "jest": "^20.0.4",
    "pre-commit": "1.2.2",
    "prettier": "^1.4.4"
