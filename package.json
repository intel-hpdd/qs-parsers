{
  "name": "@mfl/qs-parsers",
  "version": "4.0.0",
  "description": "A set of parsers useful for parsing input <-> qs",
  "main": "source/index.js",
  "scripts": {
    "eslint": "eslint ./",
    "test": "jest",
    "cover": "NODE_ENV=test yarn test -- --coverage",
    "flow": "flow"
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "eslint",
    "flow",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:MFL/qs-parsers.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/setup-jasmine-env.js",
      "/test"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@mfl)/"
    ]
  },
  "author": "MFL Team",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.1.8",
    "eslint": "3.15.0",
    "eslint-config-prettier": "1.2.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-prettier": "2.0.0",
    "flow-bin": "0.39.0",
    "@mfl/flow-jasmine": "1.6.0",
    "@mfl/jasmine-n-matchers": "2.1.1",
    "jasmine-reporters": "2.2.0",
    "jest": "18.1.0",
    "pre-commit": "1.2.2",
    "prettier": "0.18.0"
  },
  "dependencies": {
    "@mfl/fp": "7.0.8",
    "@mfl/parsely": "4.0.0"
  }
}
